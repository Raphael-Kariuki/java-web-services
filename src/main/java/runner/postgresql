ARG DEBIAN_FRONTEND=noninteractive
ARG DEBIAN_FRONTEND
ARG LOG_LEVEL=trace
ARG LOG_LEVEL

FROM postgres
RUN localedef -i en_US -c -f UTF-8 -A /usr/share/locale/locale.alias en_US.UTF-8
ENV LANG en_US.utf8

